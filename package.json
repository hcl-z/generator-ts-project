{
  "name": "generator-ts-project",
  "version": "0.0.4",
  "description": "generating a project with tsup, biome, vitest",
  "author": "hcl-z",
  "license": "MIT",
  "homepage": "https://github.com/hcl-z/generator-tsp",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hcl-z/generator-tsp.git"
  },
  "keywords": [
    "yeoman-generator",
    "generator",
    "tsup",
    "vitest",
    "biome"
  ],
  "main": "generators/index.js",
  "files": [
    "generators"
  ],
  "scripts": {
    "pretest": "eslint .",
    "release": "bumpp"
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.20.0",
    "chalk": "^2.1.0",
    "inquirer-npm-name": "^4.0.0",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "yeoman-generator": "5.10.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "bumpp": "^9.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^3.0.9",
    "jest": "^26.1.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "release-it": "^17.3.0",
    "tsup": "^8.1.0",
    "vitest": "^0.34.5",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.7.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error",
      "no-await-in-loop": "off"
    },
    "plugins": [
      "prettier"
    ]
  }
}

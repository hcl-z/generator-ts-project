{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Yeoman Generator",
      "runtimeExecutable": "yo",
      "args": ["tsp"],
      "stopOnEntry": false,
      "cwd": "${workspaceFolder}",
      "runtimeArgs": ["--inspect-brk=9229"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
